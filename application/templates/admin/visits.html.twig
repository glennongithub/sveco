{% extends 'base.html.twig' %}
{% block javascripts %}

{% endblock %}

{% block title %}
    {% trans %} All_visits {% endtrans %}
{% endblock %}
{% block body %}
    <h1>
        {% trans %} All_visits {% endtrans %}
    </h1>
    Here comes a list of visits

    <ul>
        {% for visit in visits %}
            <li>
                Date: {{ visit.visitdate|date('Y-m-d') }}, status: {{ visit.status }}, note: {{ visit.note }}
            </li>
        {% endfor %}
    </ul>


    <br>
